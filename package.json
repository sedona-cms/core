{
  "name": "@sedona-cms/core",
  "version": "1.2.3",
  "description": "",
  "repository": {
    "type": "git",
    "url": "git@github.com:sedona-cms/core.git"
  },
  "bugs": {
    "url": "https://github.com/sedona-cms/blocks/issues"
  },
  "main": "lib/index.js",
  "types": "types/index.d.ts",
  "scripts": {
    "dev": "nuxt --config-file dev/nuxt.config.js",
    "dev:build": "nuxt build --config-file dev/nuxt.config.js",
    "dev:build:analyze": "nuxt build --analyze --config-file dev/nuxt.config.js",
    "dev:start": "nuxt start --config-file dev/nuxt.config.js",
    "watch": "make -f Makefile watch",
    "build": "make -f Makefile build-prod",
    "eslint": "eslint --fix '{src,scripts}/**/*.{js,ts,tsx}'",
    "test": "npm run test:unit",
    "test:unit": "jest --color --coverage"
  },
  "keywords": [],
  "author": "Ivan Kalinichenko <kalinichenko@constlab.ru>",
  "license": "MIT",
  "dependencies": {
    "nanoid": "^3.1.5",
    "quasar": "^1.10.5"
  },
  "devDependencies": {
    "@babel/core": "^7.9.6",
    "@babel/preset-env": "^7.9.6",
    "@babel/preset-typescript": "^7.9.0",
    "@nuxt/http": "^0.5.1",
    "@nuxt/types": "^0.7.5",
    "@nuxtjs/bulma": "^1.2.7",
    "@sedona-cms/eslint-config": "^1.0.1",
    "@types/jest": "^25.2.1",
    "@types/node": "^13.13.5",
    "@vue/babel-helper-vue-jsx-merge-props": "^1.0.0",
    "@vue/babel-preset-jsx": "^1.1.2",
    "@vue/test-utils": "^1.0.2",
    "babel-jest": "^25.5.1",
    "husky": "^4.2.5",
    "jest": "^25.5.4",
    "lint-staged": "^10.2.2",
    "npm-self-link": "^1.1.7",
    "nuxt": "^2.12.2",
    "postcss": "^7.0.29",
    "postcss-prefixwrap": "^1.15.0",
    "postcss-remove-rules": "^1.0.0",
    "prettier": "2.0.1",
    "tsc-watch": "^4.2.3",
    "typescript": "^3.8.3",
    "web-streams-polyfill": "^2.1.1"
  },
  "jest": {
    "testRegex": "(__tests__/.*|(\\.|/)(test|spec))\\.(jsx?|tsx?)$"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "src/**/*.{js,ts,tsx,vue,md}": [
      "prettier --write"
    ],
    "src/**/*.{js,ts,tsx}": [
      "prettier --write",
      "npm run eslint"
    ],
    "dev/**/*.{js,vue,json}": [
      "prettier --write"
    ],
    "scripts/*.js": [
      "prettier --write",
      "npm run eslint"
    ]
  }
}
